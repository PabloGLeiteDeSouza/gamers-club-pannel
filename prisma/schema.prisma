generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users{
  id Int @id @default(autoincrement())
  nome String? @db.Text
  usuario String @unique @db.Text
  email String @unique @db.Text
  password String? @db.Text
  private_key String? @db.Text
  public_key String? @db.Text
  createdAt DateTime @db.Date
  updatedAt DateTime @db.Date @default(now())
  discord_id String? @unique @db.Text
  discord_username String? @db.Text
  discord_avatar String? @db.Text
  object_profile_discord String? @db.Text
}